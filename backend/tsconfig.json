{
		"compilerOptions": {
			"outDir": "./dist/",
			"noImplicitAny": true,
			"module": "commonjs",
			"target": "es5",
			"allowSyntheticDefaultImports": true,
			"lib": [
					"es2017"
			],
			"types": [
					"node", "express", "morgan", "helmet", "cors"
			]
		},
		"include": [
				"src/**/*"
		],
		"exclude": [
				"node_modules"
		]
	}